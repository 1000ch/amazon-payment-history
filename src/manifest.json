{
  "manifest_version": 2,
  "name": "Amazon Payment History",
  "short_name": "APH",
  "version": "1.1.1",
  "description": "Amazon Payment History.",
  "author": "1000ch",
  "permissions": [
    "background",
    "storage",
    "tabs",
    "https://www.amazon.co.jp/gp/css/order-history"
  ],
  "browser_action": {
    "default_title": "Amazon Payment History"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "offline_enabled": false
}
